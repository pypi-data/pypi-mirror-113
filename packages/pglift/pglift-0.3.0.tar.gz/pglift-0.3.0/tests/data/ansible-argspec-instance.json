{
  "configuration": {
    "required": false,
    "type": "dict"
  },
  "name": {
    "required": true,
    "type": "str"
  },
  "port": {
    "description": [
      "TCP port the postgresql instance will be listening to"
    ],
    "type": "int"
  },
  "prometheus_port": {
    "default": 9187,
    "description": [
      "TCP port for the web interface and telemetry of Prometheus"
    ],
    "type": "int"
  },
  "ssl": {
    "default": false,
    "required": false,
    "type": "bool"
  },
  "state": {
    "choices": [
      "stopped",
      "started",
      "absent"
    ],
    "default": "started",
    "description": [
      "Runtime state"
    ]
  },
  "version": {
    "description": [
      "PostgreSQL version"
    ],
    "type": "str"
  }
}